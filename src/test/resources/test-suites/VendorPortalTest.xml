<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="vendor-portal" parallel="tests" thread-count="4">
    <test name="user-sam">
        <parameter name="testDataPath" value="test-data/vendorportal/testdataSam.json"/>
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.automation.tests.vendorportal.VendorPortalTest"/>
        </classes>
    </test>
    <test name="user-mike">
        <parameter name="testDataPath" value="test-data/vendorportal/testdataMike.json"/>
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.automation.tests.vendorportal.VendorPortalTest"/>
        </classes>
    </test>
    <test name="user-john">
        <parameter name="testDataPath" value="test-data/vendorportal/testdataJohn.json"/>
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.automation.tests.vendorportal.VendorPortalTest"/>
        </classes>
    </test>
</suite>

